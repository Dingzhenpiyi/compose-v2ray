{
 "log": {
  "loglevel": "warning",
  "access": "/var/log/v2ray/access.log",
  "error": "/var/log/v2ray/error.log"
 },
  "inbounds": [
    {
      "port": 10000, //与 caddy里对应
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "你生成的uuid",
            "alterId": 0 //新版改为0
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/v2ray&$"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
